{
    "compilerOptions": {
        "tsBuildInfoFile": "_cache/.tsbuildinfo",
        "composite": true,
        "target": "esnext",
        "moduleResolution": "node",
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": true,
        "rootDir": "src",
        "outDir": "dist",
        "strict": true,
        "stripInternal": true
    },
    "include": [
        "src"
    ]
}